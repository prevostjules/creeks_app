<div id="header" class="banner" style="background-image: linear-gradient(rgba(0,0,0,0.4),rgba(0,0,0,0.4)), url('<%= cl_image_path "https://res.cloudinary.com/dvwipbafx/image/upload/v1593001009/banner_nihvgl.png", height: 300, width: 400, crop: :fill %>');">
    <div class="container">
      <div class="row">
        <div class="col-12">
          <h1 class="brand-h1">Creeks.App</h1>
          <p>
            <span id="banner-typed-text"></span>
          </p>
        </div>
        <%= form_tag pages_path, method: :get do %>
          <%= text_field_tag :query,
            params[:query],
            class: "form-control form-text my-1 col-sm-12",
            placeholder: "Rechercher" %>
          <%= submit_tag "Search", class: "btn btn-primary" %>
        <% end %>
      </div>
    </div>
</div>


